@prefix : <http://purl.org/ejp-rd/metadata-model/v1/example-rdf/> .
@prefix dcat:  <http://www.w3.org/ns/dcat#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix ejp:   <http://purl.org/ejp-rd/vocabulary/> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix sio:  <http://semanticscience.org/resource/> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ordo: <http://www.orpha.net/ORDO/> .
@prefix loc: <http://localhost/>.

loc:new a ejp:Biobank ;
  dct:isPartOf <{{&parent_url}}>;
  dct:hasVersion "1";
  dct:publisher :organisation ;        
  dct:title "{{title}}" ;
  dct:description "{{description}}" ;
  ejp:populationCoverage  :population_coverage ;
  dcat:theme  {{&themes}} ;
  foaf:page {{&pages}} .

:organisation a foaf:Organisation ;
  dct:title "{{publisher}}" ;
  foaf:name "{{publisher}}" ;
  dct:spatial  :location .

:population_coverage  a sio:SIO_001166 ;
  rdfs:label  "{{populationcoverage}}" .

:location a dct:Location ;
  dct:title  "tmp" .